#MainWindow, #centralwidget {
    background: #121212;
}

#plot_bg {
    background-color: qradialgradient(spread:pad, cx:0.5, cy:0.5, radius:0.9, fx:0.5, fy:0.5, stop:0 rgba(0,0,0,1), stop:0.66 rgba(2,0,36,1), stop:0.85 rgba(45,5,85,1), stop:1 rgba(137,3,147,1));
}

#centralwidget, #plot_bg {
    border-radius: 7px;
}

#plot, QCustomPlot {
    border-bottom-left-radius: 6px;
    border-bottom-right-radius: 6px;
}

QRadioButton {

    min-height: 23px;
    max-height: 30px;
    min-width: 23px;
    max-width: 30px;

    width: 30px;
    height: 23px;
    border: none;
    color: #121212;
}

QRadioButton:indicator {
    min-width: 0px;
    min-height: 0px;
    max-width: 0px;
    max-height: 0px;
    width: 0px;
    height: 0px;
}

QRadioButton:checked {
    color: #121212;
    background: rgba(255, 255, 255, 10);
    background-color: rgba(255, 255, 255, 10);
}

QRadioButton:unchecked {
    color: #121212;
    background: rgba(255, 255, 255, 2);
    background-color: rgba(255, 255, 255, 2);
}

#x_mode1:unchecked {
    image: url(:/svg/x_mode1);
}

#x_mode1:checked {
    image: url(:/svg/x_mode1);
}

#x_mode2:unchecked {
    image: url(:/svg/x_mode2);

}

#x_mode2:checked {
    image: url(:/svg/x_mode2);
}

#inputs {
    background-color: rgba(255,255,255,10);
}

QLineEdit {
    max-height: 23px;
    min-height: 23px;
    border: none;
    background-color: rgba(255, 255, 255, 0);
    font-size: 14px;
    color: #e5e5e5;
}

#x_input1 {
    border-bottom: #ff0000 1px solid;
}

#x_label1, #x_label2 {
    width: 38px;
    background-color: rgba(255,255,255,10);
}

#x_label1 {
    background-image: url(:/svg/label_mode1);
}

#x_label2 {
    background-image: url(:/svg/label_mode2);
}

#display {
    min-height: 63px;
    background: transparent;
    padding-right: 3px;
    color: #E5E5E5;
}
