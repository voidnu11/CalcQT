
#menubar {
    max-height: 0px;
}

QFrame {
    border: none;
}

QObject[objectName^="btn"],
QObject[objectName^="btn"]:hover {
    background: rgba(255, 255, 255, 10);
}

QObject[objectName^="btn"]:pressed {
    background: rgba(255, 255, 255, 20);
}

QObject[objectName^="pbtn"],
QObject[objectName^="pbtn"]:hover {
    background: rgba(255, 255, 255, 50);
}

QObject[objectName^="pbtn"]:pressed {
    background: rgba(255, 255, 255, 60);
}

QObject[objectName^="rbtn"],
QObject[objectName^="rbtn"]:hover {
    background: #311E44;
}

QObject[objectName^="rbtn"]:pressed {
    background: #220A34;
}

QObject[objectName^="hbtn"],
QObject[objectName^="btn"],
QObject[objectName^="btn"]:hover,
QObject[objectName^="btn"]:pressed,
QObject[objectName^="pbtn"],
QObject[objectName^="pbtn"]:hover,
QObject[objectName^="pbtn"]:pressed,
QObject[objectName^="rbtn"],
QObject[objectName^="rbtn"]:hover,
QObject[objectName^="rbtn"]:pressed {
    background-position: 50%;
    color: transparent;
    border: none;
}

QObject[objectName^="hbtn"] {
    max-height: 0px;
    max-width: 0px;
}

