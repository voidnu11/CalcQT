#rbtn_div {
    background-image: url(:/svg/div);
    background-color: qlineargradient(spread:pad, x1:0, y1:0, x2:1, y2:1, stop:0 rgba(24, 11, 53, 255), stop:0.824 rgba(54, 10, 45, 255), stop:1 rgba(55, 10, 40, 255));
}

#rbtn_div:pressed {
    background-color: qlineargradient(spread:pad, x1:0, y1:0, x2:1, y2:1, stop:0 rgba(24, 11, 53, 200), stop:0.824 rgba(54, 10, 45, 200), stop:1 rgba(55, 10, 40, 200));
